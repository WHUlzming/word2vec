public   void   setTargetColors  (  String  [  ]  targetColors  )  { 

this  .  targetColors  =  new   Color  [  targetColors  .  length  ]  ; 

for  (  int   i  =  0  ;  i  <  targetColors  .  length  ;  i  ++  )  { 

try  { 

this  .  targetColors  [  i  ]  =  new   Color  (  Integer  .  parseInt  (  targetColors  [  i  ]  ,  16  )  )  ; 

}  catch  (  Exception   ex  )  { 

} 

} 

} 

