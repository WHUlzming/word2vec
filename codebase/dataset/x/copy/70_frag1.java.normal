private   static   void   writeBytes  (  File   file  ,  byte  [  ]  data  )  { 

try  { 

OutputStream   fos  =  new   FileOutputStream  (  file  )  ; 

OutputStream   os  =  new   BufferedOutputStream  (  fos  )  ; 

os  .  write  (  data  )  ; 

os  .  close  (  )  ; 

}  catch  (  Exception   e  )  { 

e  .  printStackTrace  (  )  ; 

} 

} 

