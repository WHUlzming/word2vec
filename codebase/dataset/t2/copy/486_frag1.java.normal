public   static   void   setCharset  (  IFile   file  )  { 

if  (  file  ==  null  )  { 

return  ; 

} 

try  { 

file  .  setCharset  (  "UTF-8"  ,  new   NullProgressMonitor  (  )  )  ; 

}  catch  (  CoreException   e  )  { 

ModelDiagramEditorPlugin  .  getInstance  (  )  .  logError  (  "Unable to set charset for file "  +  file  .  getFullPath  (  )  ,  e  )  ; 

} 

} 

